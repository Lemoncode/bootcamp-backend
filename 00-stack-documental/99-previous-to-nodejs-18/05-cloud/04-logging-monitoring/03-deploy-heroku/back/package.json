{
  "name": "01-config",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "sh ./create-dev-env.sh",
    "start": "run-p -l type-check:watch start:dev start:local-db",
    "start:dev": "nodemon --exec babel-node --extensions \".ts\" src/index.ts",
    "prestart:console-runners": "npm run prestart",
    "start:console-runners": "run-p type-check:watch console-runners start:local-db",
    "console-runners": "npm run type-check && nodemon --no-stdin --exec babel-node -r dotenv/config --extensions \".ts\" src/console-runners/index.ts",
    "start:local-db": "docker-compose up -d || echo \"Fail running docker-compose up, do it manually!\"",
    "remove:local-db": "docker-compose down || echo \"Fail running docker-compose down, do it manually!\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest -c ./config/test/jest.js",
    "test:watch": "npm run test -- --watchAll -i",
    "clean": "rimraf dist",
    "build": "npm run type-check && npm run clean && npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production babel src -d dist --extensions \".ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.154.0",
    "@aws-sdk/s3-request-presigner": "^3.157.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.9.0",
    "rollbar": "^2.25.2",
    "triple-beam": "^1.3.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@shelf/jest-mongodb": "^4.1.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "inquirer": "^8.2.4",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "typescript": "^4.7.4"
  }
}
