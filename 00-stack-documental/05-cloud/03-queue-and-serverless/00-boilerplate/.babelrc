{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "16"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "common": "./src/common",
          "common-app": "./src/common-app",
          "core": "./src/core",
          "dals": "./src/dals",
          "pods": "./src/pods"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    },
    "production": {
      "ignore": ["**/*.spec.ts", "./src/console-runners"]
    }
  }
}
